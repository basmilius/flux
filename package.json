{
    "name": "fancee-flux-workspace",
    "private": true,
    "scripts": {
        "build": "pnpm run -C packages/flux build",
        "dev": "pnpm run -C packages/flux dev",
        "docs:build": "pnpm run -C packages/docs build",
        "docs:dev": "pnpm run -C packages/docs dev",
        "link": "pnpm link --dir packages/flux --global",
        "publish": "pnpm --filter '@fancee/*' -r publish --access restricted --no-git-checks"
    },
    "engines": {
        "node": ">=20"
    },
    "devDependencies": {
        "@types/luxon": "^3.4.1",
        "@types/node": "^20.11.3",
        "@vue/language-server": "^1.9.0-alpha.3"
    }
}
