{
    "name": "fancee-flux-workspace",
    "private": true,
    "scripts": {
        "build": "pnpm --filter '@fancee/*' -r build",
        "dev": "pnpm --filter '@fancee/*' -r dev",
        "docs:build": "pnpm run -C packages/docs build",
        "docs:dev": "pnpm run -C packages/docs dev",
        "link:2": "pnpm link --dir packages/vue2 --global",
        "link:3": "pnpm link --dir packages/vue3 --global",
        "publish": "pnpm --filter '@fancee/*' -r publish --access restricted --no-git-checks"
    },
    "engines": {
        "node": ">=20"
    },
    "devDependencies": {
        "@types/luxon": "^3.3.0",
        "@types/node": "^20.1.7",
        "vite": "^4.3.7",
        "vue": "^3.3.2"
    }
}
