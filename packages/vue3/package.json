{
    "name": "@fancee/flux",
    "version": "1.1.0",
    "type": "module",
    "scripts": {
        "build": "vite build && pnpm run gentypes",
        "dev": "vite build --watch",
        "gentypes": "pnpm gentypes:dts && pnpm gentypes:webtypes",
        "gentypes:dts": "vue-tsc --project ../flux/tsconfig.json --outDir dist && tsc-alias -p ../flux/tsconfig.json --dir ../vue3/dist",
        "gentypes:webtypes": "vue-docgen-web-types --configFile ../../web-types-gen.config.json"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/fancee.flux.js",
            "require": "./dist/fancee.flux.umd.cjs"
        },
        "./style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/fancee.flux.umd.cjs",
    "module": "./dist/fancee.flux.js",
    "types": "./dist/index.d.ts",
    "web-types": "./dist/web-types.json",
    "dependencies": {
        "vue-demi": "^0.14.6"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-common-types": "^6.4.2",
        "@vitejs/plugin-vue": "^4.3.3",
        "@vue/compiler-sfc": "^3.3.4",
        "autoprefixer": "^10.4.15",
        "luxon": "^3.4.2",
        "minimist": "^1.2.8",
        "tsc-alias": "^1.8.7",
        "typescript": "^5.2.2",
        "vite": "^4.4.9",
        "vue": "^3.3.4",
        "vue-docgen-web-types": "^0.1.8",
        "vue-tsc": "^1.8.8"
    },
    "peerDependencies": {
        "luxon": "^3.4.2",
        "vue": "^3.3.4"
    }
}
