{
    "name": "@fancee/flux",
    "version": "1.0.44",
    "type": "module",
    "scripts": {
        "build": "vue-demi-switch 3 && vite build && pnpm run gentypes",
        "dev": "vue-demi-switch 3 && vite build --watch",
        "gentypes": "pnpm gentypes:dts && pnpm gentypes:webtypes && pnpm gentypes:clean",
        "gentypes:clean": "mv dist/flux/src/* dist && rm -rf dist/flux dist/vue3",
        "gentypes:dts": "vue-tsc --declaration --lib esnext,dom --outDir dist --emitDeclarationOnly --moduleResolution node --target esnext index.ts",
        "gentypes:webtypes": "vue-docgen-web-types --configFile ../../web-types-gen.config.json"
    },
    "exports": {
        ".": {
            "import": "./dist/fancee.flux.js",
            "require": "./dist/fancee.flux.umd.cjs",
            "types": "./dist/index.d.ts"
        },
        "./style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/fancee.flux.umd.cjs",
    "module": "./dist/fancee.flux.js",
    "types": "./dist/index.d.ts",
    "web-types": "dist/web-types.json",
    "dependencies": {
        "vue-demi": "^0.14.0"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-common-types": "^6.4.0",
        "@vitejs/plugin-vue": "^4.2.1",
        "@vue/compiler-sfc": "^3.2.47",
        "autoprefixer": "^10.4.14",
        "luxon": "^3.3.0",
        "minimist": "^1.2.8",
        "pinia": "^2.0.35",
        "typescript": "^4.9.5",
        "vite": "^4.3.5",
        "vue": "^3.2.47",
        "vue-docgen-web-types": "^0.1.8",
        "vue-tsc": "1.6.1"
    },
    "peerDependencies": {
        "luxon": "^3.3.0",
        "pinia": "^2.0.35",
        "vue": "^3.2.47"
    }
}
