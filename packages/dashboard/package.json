{
    "name": "@basmilius/flux-dashboard",
    "version": "1.4.0",
    "type": "module",
    "scripts": {
        "build": "vite build && pnpm run gentypes",
        "dev": "vite build --watch",
        "gentypes": "vue-tsc --outDir dist && tsc-alias --dir dist",
        "prepublishOnly": "cp ../../README.md ."
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/basmilius.flux-dashboard.js",
            "require": "./dist/basmilius.flux-dashboard.umd.cjs"
        },
        "./style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/basmilius.flux-dashboard.umd.cjs",
    "module": "./dist/basmilius.flux-dashboard.js",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "dependencies": {
        "@basmilius/flux": "workspace:*",
        "vue": "catalog:"
    },
    "devDependencies": {
        "@types/node": "catalog:",
        "@vitejs/plugin-vue": "catalog:",
        "css-class-generator": "catalog:",
        "sass-embedded": "catalog:",
        "tsc-alias": "catalog:",
        "typescript": "catalog:",
        "vite": "catalog:",
        "vite-css-modules": "catalog:",
        "vue-tsc": "catalog:"
    }
}
