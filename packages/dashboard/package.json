{
    "name": "@basmilius/flux-dashboard",
    "version": "1.4.0",
    "type": "module",
    "scripts": {
        "build": "vite build && pnpm run gentypes",
        "dev": "vite build --watch",
        "gentypes": "pnpm gentypes:dts && pnpm gentypes:webtypes",
        "gentypes:dts": "vue-tsc --outDir dist && tsc-alias --dir dist",
        "gentypes:webtypes": "vue-docgen-web-types --configFile web-types-gen.config.json"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/basmilius.flux.dashboard.js",
            "require": "./dist/basmilius.flux.dashboard.umd.cjs"
        },
        "./style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/basmilius.flux.dashboard.umd.cjs",
    "module": "./dist/basmilius.flux.dashboard.js",
    "types": "./dist/index.d.ts",
    "web-types": "./dist/web-types.json",
    "dependencies": {
        "@basmilius/flux": "workspace:*"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.1.3",
        "@vue/compiler-sfc": "^3.4.38",
        "css-class-generator": "^2.0.0",
        "sass-embedded": "^1.77.8",
        "typescript": "^5.5.4",
        "vite": "^5.4.2",
        "vite-css-modules": "^1.4.2",
        "vue": "^3.4.38"
    },
    "peerDependencies": {
        "vue": "^3.4.38"
    }
}
