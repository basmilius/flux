{
    "name": "@fancee/flux-vue2",
    "version": "1.0.59",
    "type": "module",
    "scripts": {
        "build": "vite build && pnpm run gentypes",
        "dev": "vite build --watch",
        "gentypes": "pnpm gentypes:dts && pnpm gentypes:webtypes",
        "gentypes:dts": "vue-tsc --project ../flux/tsconfig.json --outDir dist && tsc-alias -p ../flux/tsconfig.json --dir ../vue2/dist",
        "gentypes:webtypes": "vue-docgen-web-types --configFile ../../web-types-gen.config.json"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/fancee.flux.js",
            "require": "./dist/fancee.flux.umd.cjs"
        },
        "./style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/fancee.flux.umd.cjs",
    "module": "./dist/fancee.flux.js",
    "types": "./dist/index.d.ts",
    "web-types": "./dist/web-types.json",
    "dependencies": {
        "vue-demi": "^0.14.5"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-common-types": "^6.4.0",
        "@vitejs/plugin-vue2": "^2.2.0",
        "autoprefixer": "^10.4.14",
        "luxon": "^3.3.0",
        "minimist": "^1.2.8",
        "tsc-alias": "^1.8.6",
        "typescript": "^5.0.4",
        "vite": "^4.3.8",
        "vue": "^2.7.14",
        "vue-docgen-web-types": "^0.1.8",
        "vue-template-compiler": "^2.7.14",
        "vue-tsc": "^1.7.8"
    },
    "peerDependencies": {
        "luxon": "^3.3.0",
        "vue": "^2.7.14"
    }
}
