{
    "name": "@basmilius/flux",
    "version": "1.4.0",
    "type": "module",
    "scripts": {
        "build": "vite build && pnpm run gentypes",
        "dev": "vite build --watch",
        "gentypes": "vue-tsc --outDir dist && tsc-alias --dir dist",
        "prepublishOnly": "cp ../../README.md ."
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/basmilius.flux.js",
            "require": "./dist/basmilius.flux.umd.cjs"
        },
        "./style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/basmilius.flux.umd.cjs",
    "module": "./dist/basmilius.flux.js",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "dependencies": {
        "clsx": "^2.1.1",
        "imask": "^7.6.1",
        "lodash-es": "^4.17.21",
        "luxon": "^3.5.0",
        "vue": "^3.5.2"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-common-types": "^6.6.0",
        "@types/lodash-es": "^4.17.12",
        "@types/luxon": "^3.4.2",
        "@types/node": "^22.5.3",
        "@vitejs/plugin-vue": "^5.1.3",
        "css-class-generator": "^2.0.0",
        "sass-embedded": "^1.78.0",
        "tsc-alias": "^1.8.10",
        "typescript": "^5.5.4",
        "vite": "^5.4.3",
        "vite-css-modules": "^1.4.2",
        "vue-tsc": "^2.1.6"
    }
}
