{
    "name": "@basmilius/flux",
    "version": "1.4.14",
    "type": "module",
    "license": "MIT",
    "funding": "https://github.com/sponsors/basmilius",
    "homepage": "https://flux.bas.dev",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/basmilius/flux.git",
        "directory": "packages/flux"
    },
    "keywords": [
        "ui libeary",
        "component library",
        "design system",
        "vue",
        "vue 3",
        "ui",
        "components",
        "flux"
    ],
    "scripts": {
        "build": "vite build && pnpm run gentypes",
        "dev": "vite build --watch",
        "gentypes": "vue-tsc --outDir dist && tsc-alias --dir dist",
        "prepublishOnly": "cp ../../README.md ."
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/flux.js",
            "require": "./dist/flux.umd.cjs"
        },
        "./style.css": "./dist/style.css"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "files": [
        "dist"
    ],
    "main": "./dist/flux.umd.cjs",
    "module": "./dist/flux.js",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "dependencies": {
        "clsx": "^2.1.1",
        "imask": "^7.6.1",
        "lodash-es": "^4.17.21",
        "luxon": "^3.5.0",
        "vue": "^3.5.9"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-common-types": "^6.6.0",
        "@types/lodash-es": "^4.17.12",
        "@types/luxon": "^3.4.2",
        "@types/node": "^22.7.2",
        "@vitejs/plugin-vue": "^5.1.4",
        "css-class-generator": "^2.0.0",
        "sass-embedded": "^1.79.3",
        "tsc-alias": "^1.8.10",
        "typescript": "^5.6.2",
        "vite": "^5.4.8",
        "vite-css-modules": "^1.4.2",
        "vue-tsc": "^2.1.6"
    }
}
