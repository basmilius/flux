{
    "name": "@fancee/flux",
    "version": "1.2.1",
    "type": "module",
    "scripts": {
        "build": "vite build && pnpm run gentypes",
        "dev": "vite build --watch",
        "gentypes": "pnpm gentypes:dts && pnpm gentypes:webtypes",
        "gentypes:dts": "vue-tsc --outDir dist && tsc-alias --dir dist",
        "gentypes:webtypes": "vue-docgen-web-types --configFile web-types-gen.config.json"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/fancee.flux.js",
            "require": "./dist/fancee.flux.umd.cjs"
        },
        "./style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/fancee.flux.umd.cjs",
    "module": "./dist/fancee.flux.js",
    "types": "./dist/index.d.ts",
    "web-types": "./dist/web-types.json",
    "dependencies": {
        "imask": "^7.1.3"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-common-types": "^6.5.1",
        "@vitejs/plugin-vue": "^4.5.1",
        "@vue/compiler-sfc": "^3.3.10",
        "autoprefixer": "^10.4.16",
        "luxon": "^3.4.4",
        "minimist": "^1.2.8",
        "tsc-alias": "^1.8.8",
        "typescript": "^5.3.2",
        "vite": "^5.0.5",
        "vue": "^3.3.10",
        "vue-docgen-web-types": "^0.1.8",
        "vue-tsc": "^1.9.0-alpha.3"
    },
    "peerDependencies": {
        "luxon": "^3.4.4",
        "vue": "^3.3.10"
    }
}
